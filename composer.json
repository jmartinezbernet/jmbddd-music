{
  "name": "jmartinezbernet/jmbddd-music",
  "description": "Example of an app developed under DDD principles",
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "ramsey/uuid": "^3.7",
    "psr/container": "^1.0",
    "psr/http-message": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Song\\": "Song/src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2"
  },
  "autoload-dev": {
    "psr-4": {
      "SongTest\\": "Song/tests/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "preferred-install": "dist"
  }
}